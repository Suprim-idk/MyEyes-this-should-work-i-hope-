entrypoint = "main.py"
modules = ["python-3.11"]

[nix]
channel = "stable-25_05"
packages = ["espeak-ng", "libGL", "libGLU", "libjpeg_turbo", "libpng", "which", "espeak"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Navigation System"

[[workflows.workflow]]
name = "Navigation System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python pytthon"

[workflows.workflow.metadata]
outputType = "console"
